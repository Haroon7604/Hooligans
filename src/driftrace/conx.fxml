<AnchorPane id="AnchorPane" prefHeight="429.0" prefWidth="611.0" stylesheets="@../views/css1.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="driftrace.ConxController">
